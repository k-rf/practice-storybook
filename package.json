{
  "name": "practice-storybook",
  "version": "1.0.0",
  "description": "The practice of Storybook with React.",
  "author": "Kawano Kenichi <kenichi.k.820@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --cache --ext .tsx,.ts,.js ./src",
    "format": "prettier . --write",
    "check:types": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest",
    "test:snapshot": "jest --config jest.config.storyshots.ts",
    "storybook": "start-storybook -p 6006"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
